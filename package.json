{
  "name": "url-shortening-api",
  "version": "1.0.0",
  "description": "An api that basically helps you shorten a lengthy url",
  "main": "index.js",
  "homepage": "http://mctursh.github.io/url-shortening-app",
  "scripts": {
    "start": "react-scripts start",
    "dev": "parcel public/index.html -p 3000",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "author": "Adenuga Ayoade Henry (McTursh)",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.2.1",
    "v8-compile-cache": "^2.3.0"
  }
}
